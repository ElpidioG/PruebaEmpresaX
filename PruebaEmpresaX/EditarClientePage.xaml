<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PruebaEmpresaX.EditarClientePage">
    <StackLayout>
            <Label Text="Editar Cliente" FontSize="Large" HorizontalOptions="Center" Margin="0,10,0,20" />
            <Entry x:Name="idEntry" Placeholder="Id" IsEnabled="False" />
            <Entry x:Name="nombreEntry" Placeholder="Nombre" />
            <Entry x:Name="apellidoEntry" Placeholder="Apellido" />
            <Entry x:Name="telefonoEntry" Placeholder="Teléfono" Keyboard="Telephone" />
            <Button x:Name="GuardarCambios"  Text="Guardar Cambios" Clicked="GuardarCambios_Clicked_1" />
        </StackLayout>

</ContentPage>